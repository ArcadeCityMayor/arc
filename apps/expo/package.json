{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "expo start -c",
    "clean": "rm -rf .expo .turbo node_modules",
    "android": "TAMAGUI_TARGET=native yarn expo run:android",
    "android:release": "TAMAGUI_TARGET=native yarn expo run:android --variant release",
    "ios": "TAMAGUI_TARGET=native yarn expo run:ios",
    "ios:release": "TAMAGUI_TARGET=native yarn expo run:ios --configuration Release",
    "web": "TAMAGUI_TARGET=web yarn expo start --web",
    "eject": "expo eject",
    "build:ios": "eas build -p ios",
    "build:ios:local": "eas build -p ios --local",
    "build:android": "eas build -p android",
    "build:android:apk": "eas build -p android --profile preview",
    "build:android:apk:local": "eas build -p android --profile preview --local",
    "build:android:dev-client": "eas build -p android --profile development",
    "apk": "yarn build:android:apk",
    "doctor": "npx expo-doctor",
    "start": "expo start --dev-client"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@bacons/expo-metro-runtime": "^2.0.1",
    "@expo-google-fonts/source-code-pro": "^0.2.3",
    "@my/ui": "*",
    "@shopify/flash-list": "1.4.0",
    "app": "*",
    "babel-plugin-module-resolver": "^4.1.0",
    "burnt": "^0.10.0",
    "expo": "~48.0.9",
    "expo-auth-session": "~4.0.3",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-font": "~11.1.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-linking": "~4.0.1",
    "expo-router": "1.0.0-rc1",
    "expo-secure-store": "~12.1.1",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.4",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "^13.4.0",
    "react-native-web": "~0.18.10"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@expo/metro-config": "^0.7.0",
    "@tamagui/babel-plugin": "1.9.16",
    "@types/react-native-dotenv": "^0.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "react-native-dotenv": "^3.4.8",
    "typescript": "4.9.4"
  }
}
